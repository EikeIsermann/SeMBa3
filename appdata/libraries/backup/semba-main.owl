###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#exemplifiedBy
semba:exemplifiedBy rdf:type owl:ObjectProperty ;
                    owl:inverseOf semba:isExampleFor .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#follows
semba:follows rdf:type owl:ObjectProperty ;
              owl:inverseOf semba:preceeds ;
              rdf:type owl:TransitiveProperty .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#followsDirectly
semba:followsDirectly rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf semba:follows ;
                      owl:inverseOf semba:preceedsDirectly .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#hasMediaItem
semba:hasMediaItem rdf:type owl:ObjectProperty ;
                   owl:propertyChainAxiom ( semba:hasMediaItem
                                            semba:exemplifiedBy
                                          ) ,
                                          ( semba:hasMediaItem
                                            semba:follows
                                          ) ,
                                          ( semba:hasMediaItem
                                            semba:preceeds
                                          ) .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#isExampleFor
semba:isExampleFor rdf:type owl:ObjectProperty .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#linksToMediaItem
semba:linksToMediaItem rdf:type owl:ObjectProperty ;
                       rdfs:domain semba:CollectionItem .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#preceeds
semba:preceeds rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty .


###  http://www.hci.uni-wuerzburg.de/ontologies/semba-main.owl#preceedsDirectly
semba:preceedsDirectly rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf semba:preceeds .


